{
  "expo": {
    "name": "Friend's Poker",
    "slug": "friends-poker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2ecc71"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mn.boss.friendspoker",
      "buildNumber":"1.0.0"
    },
    "android": {
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2ecc71"
      },
      "package": "mn.boss.friendspoker",
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Poker mobile game for both iPhone and Android.",
    "githubUrl": "https://github.com/Serchin/Friends-Poker"
  }
}
